{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "dev": "node simple-server.js",
    "dev:next": "next dev",
    "build": "node scripts/debug-prisma-env.js && cross-env PRISMA_CLIENT_ENGINE_TYPE=library PRISMA_GENERATE_DATAPROXY=false prisma generate && next build",
    "vercel-build": "node scripts/debug-prisma-env.js && node scripts/fix-prisma-protocol.js && cross-env PRISMA_CLIENT_ENGINE_TYPE=library PRISMA_GENERATE_DATAPROXY=false prisma generate && next build",
    "postinstall": "cross-env PRISMA_CLIENT_ENGINE_TYPE=library PRISMA_GENERATE_DATAPROXY=false prisma generate && node db/apply-triggers.js",
    "start": "cross-env NODE_ENV=production node simple-server.js",
    "start:next": "next start",
    "restart": "taskkill /F /IM node.exe && npm run dev",
    "restart:unix": "kill -9 $(lsof -t -i:3000) && npm run dev",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "migrate:postgres": "ts-node --compiler-options {\"module\":\"CommonJS\"} scripts/migrate-to-postgres.ts",
    "db:apply-triggers": "node db/apply-triggers.js",
    "docker:verify": "node docker-env.js",
    "docker:start": "npm run docker:verify && docker-compose up -d",
    "check-db": "node scripts/check-database.js",
    "fix-deploy": "node scripts/fix-deploy.js"
  },
